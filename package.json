{
  "name": "faj-cli",
  "version": "2.0.6",
  "description": "FAJ - A powerful CLI resume builder with AI enhancement and multi-format export",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "faj": "./bin/faj"
  },
  "files": [
    "dist/",
    "bin/",
    "public/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikewang817/faj.git"
  },
  "homepage": "https://github.com/mikewang817/faj#readme",
  "bugs": {
    "url": "https://github.com/mikewang817/faj/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "resume",
    "cv",
    "resume-builder",
    "ai",
    "gemini",
    "openai",
    "developer",
    "job",
    "career",
    "pdf",
    "html",
    "markdown",
    "export"
  ],
  "author": "michael.wang817@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@react-pdf/fontkit": "^2.1.2",
    "@types/jspdf": "^1.3.3",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "crypto-js": "^4.2.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "jspdf": "^3.0.2",
    "openai": "^5.19.1",
    "ora": "^5.4.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "pdfmake": "^0.2.20",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/crypto-js": "^4.2.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
